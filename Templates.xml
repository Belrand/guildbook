                    GameTooltip:SetHyperlink(link)
                    GameTooltip:Show()
                end
            </OnHyperlinkEnter>
            <OnHyperlinkLeave>
                GameTooltip_SetDefaultAnchor(GameTooltip, UIParent)
                <!-- GameTooltip:Hide() -->
            </OnHyperlinkLeave>
        </Scripts>
    </Frame>
    
